{
    "presets": [
        "@babel/preset-env"
    ],
    "plugins": [
        "add-module-exports",
        [
            "@babel/transform-runtime",
            {
                "helpers": false,
                "regenerator": true
            }
        ]
    ],
    "env": {
        "cjs": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "node": "6"
                        }
                    }
                ]
            ],
            "plugins": [
                "add-module-exports",
                [
                    "@babel/transform-runtime",
                    {
                        "helpers": false,
                        "regenerator": true
                    }
                ]
            ]
        },
        "es": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "browsers": [
                                ">0.25%",
                                "not ie 11",
                                "not op_mini all"
                            ]
                        },
                        "modules": false
                    }
                ]
            ],
            "plugins": [
                [
                    "@babel/transform-runtime",
                    {
                        "helpers": false,
                        "regenerator": true
                    }
                ]
            ]
        },
        "umd": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "browsers": [
                                ">0.25%",
                                "not ie 11",
                                "not op_mini all"
                            ]
                        }
                    }
                ]
            ],
            "plugins": [
                "add-module-exports",
                [
                    "@babel/transform-runtime",
                    {
                        "helpers": false,
                        "regenerator": true
                    }
                ]
            ]
        },
        "test": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "node": "6"
                        }
                    }
                ]
            ],
            "plugins": [
                "add-module-exports",
                [
                    "@babel/transform-runtime",
                    {
                        "helpers": false,
                        "regenerator": true
                    }
                ]
            ]
        }
    }
}